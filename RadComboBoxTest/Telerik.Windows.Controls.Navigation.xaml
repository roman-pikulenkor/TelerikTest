                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="SplitButtonVisibility">
                    <VisualState x:Name="SplitButtonVisible"/>
                    <VisualState x:Name="SplitButtonHidden">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="SplitButton" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="headerContainer" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}">
                <telerik:RadSplitButton x:Name="SplitButton"
                        Margin="0"
                        Foreground="White"
                        Content="{TemplateBinding Header}"
                        FontSize="{TemplateBinding FontSize}"
                        ContentTemplate="{TemplateBinding HeaderTemplate}"
                        IsOpen="{Binding Path=IsPopupOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                        Style="{TemplateBinding SplitButtonStyle}"
                        Command="telerikBreadcrumb:RadBreadcrumbCommands.ClearSelectedItem">
                    <telerik:RadSplitButton.DropDownContent>
                        <ScrollViewer HorizontalScrollBarVisibility="Auto" MaxHeight="250" MaxWidth="270" MinWidth="200" Padding="0" Margin="0" BorderThickness="0" VerticalScrollBarVisibility="Auto">
                            <ItemsPresenter Margin="1"/>
                        </ScrollViewer>
                    </telerik:RadSplitButton.DropDownContent>
                </telerik:RadSplitButton>
            </Border>
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="DropDownItemControlTemplate" TargetType="telerikBreadcrumb:RadBreadcrumbBarItem">
        <Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="Mouse">
                    <VisualState x:Name="MouseEnter">
                        <Storyboard>
                            <DoubleAnimation Duration="00:00:00.3" To="1" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="OuterMouseOverBorder"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="MouseLeave"/>
                </VisualStateGroup>
                <VisualStateGroup x:Name="Selection">
                    <VisualState x:Name="Select">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="contentControl" Storyboard.TargetProperty="FontWeight">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <FontWeight>Bold</FontWeight>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="UnSelect"/>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="OuterBorder" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}"/>
            <Border x:Name="OuterMouseOverBorder" Opacity="0" Background="{telerik1:VisualStudio2013Resource ResourceKey=MouseOverBrush}"/>
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="29"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <Image Grid.Column="0"
                        Style="{StaticResource BreadcrumbImageStyle}"
                        Source="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Image}"
                        Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsIconVisible, Converter={StaticResource visibilityConverter}}"
                        Margin="-3 0 0 0"/>
                <ContentControl x:Name="contentControl"
                        Grid.Column="1"
                        FontSize="{TemplateBinding FontSize}"
                        Content="{TemplateBinding DropDownHeader}"
                        ContentTemplate="{TemplateBinding DropDownHeaderTemplate}"
                        Margin="{TemplateBinding Padding}"/>
            </Grid>
        </Grid>
    </ControlTemplate>
    <Style x:Key="RadBreadcrumbStyle" TargetType="telerikNavigation:RadBreadcrumb">
        <Setter Property="Foreground" Value="{telerik1:VisualStudio2013Resource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="{telerik1:VisualStudio2013Resource ResourceKey=BasicBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="SplitButtonStyle" Value="{StaticResource BreadcrumbSplitButtonStyle}"/>
        <Setter Property="MinWidth" Value="150"/>
        <Setter Property="OverflowButtonStyle" Value="{StaticResource BreadcrumbOverflowSplitButtonStyle}"/>
        <Setter Property="UseLayoutRounding" Value="True"/>
        <Setter Property="DropDownItemControlTemplate" Value="{StaticResource DropDownItemControlTemplate}"/>
        <Setter Property="BarItemControlTemplate" Value="{StaticResource BarItemControlTemplate}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadBreadcrumb">
                    <Grid x:Name="outerGrid">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="Visibility">
                                <VisualState x:Name="TextBoxVisibile">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="BreadcrumbBar" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <DoubleAnimation Duration="0" Storyboard.TargetName="pathTextBox" Storyboard.TargetProperty="Opacity" To="1"/>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="ItemsPresenterVisible">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0" Storyboard.TargetName="pathTextBox" Storyboard.TargetProperty="Opacity" To="0"/>
                                        <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="BreadcrumbBar" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="IsTextModeEnabled">
                                <VisualState x:Name="TextModeDisabled">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="historySplitButton" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="TextModeEnabled"/>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="15"/>
                        </Grid.ColumnDefinitions>
                        <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Grid.ColumnSpan="3"/>
                        <Path Grid.Column="0" x:Name="BackgroundShape" Data="M0,0 L20,0 L28,14 L20,28  L0,28  z" Fill="#FF00B2ED" UseLayoutRounding="False" Stretch="Fill"/>
                        <Image x:Name="image" Grid.Column="0"
                                Style="{StaticResource BreadcrumbImageStyle}"
                                Margin="0 3 5 3"
                                Source="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=CurrentContainer.Image}"
                                Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsCurrentIconVisible, Converter={StaticResource visibilityConverter}}"/>
                        <telerik:RadSplitButton x:Name="historySplitButton"
                                Style="{StaticResource BreadcrumbTextSplitButton}"
                                IsOpen="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsHistoryOpen, Mode=TwoWay}"
                                Grid.Column="1"
                                Grid.ColumnSpan="2">
                            <telerik:RadSplitButton.Content>
                                <TextBox x:Name="pathTextBox" BorderThickness="0" Margin="1" VerticalAlignment="Center" Opacity="0" FontSize="{TemplateBinding FontSize}" MinHeight="22"/>
                            </telerik:RadSplitButton.Content>
                            <telerik:RadSplitButton.DropDownContent>
                                <ListBox x:Name="HistoryListBox"
                                        BorderThickness="0"
                                        Margin="0 -1 0 0"
                                        ItemContainerStyle="{StaticResource BreadcrumbListBoxItemStyle}"
                                        ItemsSource="{TemplateBinding HistoryItems}"
                                        FontSize="{TemplateBinding FontSize}"
                                        HorizontalContentAlignment="Stretch"
                                        VerticalContentAlignment="Stretch"
                                        SelectedItem="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SelectedHistoryItem, Mode=TwoWay}"
                                        Background="{telerik1:VisualStudio2013Resource ResourceKey=AlternativeBrush}"
                                        MinHeight="24"
                                        Padding="1"/>
                            </telerik:RadSplitButton.DropDownContent>
                        </telerik:RadSplitButton>
                        <!--<local:MenuBreadcrumbBar x:Name="BreadcrumbBar"
                                Grid.Column="1"
                                Background="Transparent"
                                Foreground="{TemplateBinding Foreground}"
                                FontSize="{TemplateBinding FontSize}"
                                ItemTemplate="{TemplateBinding ItemTemplate}"
                                ItemTemplateSelector="{TemplateBinding ItemTemplateSelector}"/>-->
                        <Popup x:Name="AutocompletePopup">
                            <ListBox x:Name="AutoCompleteListBox"
                                    FontSize="{TemplateBinding FontSize}"
                                    BorderThickness="1"
                                    ItemsSource="{TemplateBinding AutocompleteItems}"
                                    MaxWidth="650"
                                    MaxHeight="500"
                                    MinWidth="200"
                                    Margin="0 -1 0 0"
                                    ItemContainerStyle="{StaticResource BreadcrumbListBoxItemStyle}"
                                    SelectedItem="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SelectedAutocompleteItem, Mode=TwoWay}"
                                    Background="{telerik1:VisualStudio2013Resource ResourceKey=AlternativeBrush}"
                                    BorderBrush="{telerik1:VisualStudio2013Resource ResourceKey=BasicBrush}"
                                    MinHeight="26"
                                    Padding="1"/>
                        </Popup>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadBreadcrumbBarItemStyle" TargetType="telerikBreadcrumb:RadBreadcrumbBarItem">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="Foreground" Value="White"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Padding" Value="3 3 0 3"/>
    </Style>
    <Style TargetType="telerikNavigation:RadBreadcrumb" BasedOn="{StaticResource RadBreadcrumbStyle}"/>
    <Style TargetType="telerikBreadcrumb:RadBreadcrumbBarItem" BasedOn="{StaticResource RadBreadcrumbBarItemStyle}"/>
    <Style TargetType="telerikNavigation:RadCoverFlow">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="TabNavigation" Value="Once"/>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <telerikNavigation:CoverFlowPanel/>
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadCoverFlow">
                    <Grid>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStateGroup">
                                <VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="DisabledBox" Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Normal"/>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
                            <telerikPrimitives:LayoutTransformControl x:Name="layoutTransform">
                                <ItemsPresenter Margin="{TemplateBinding Padding}"/>
                            </telerikPrimitives:LayoutTransformControl>
                        </Border>
                        <Rectangle x:Name="DisabledBox" Fill="{telerik1:VisualStudio2013Resource ResourceKey=ComplementaryBrush}" Visibility="Collapsed"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="telerikNavigation:RadCoverFlowItem">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadCoverFlowItem">
                    <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
                        <vsm:VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Focused"/>
                                <VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <DoubleAnimation To="0.2" Duration="0:0:0" Storyboard.TargetName="ContentElement" Storyboard.TargetProperty="Opacity"/>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="MouseOver"/>
                                <VisualState x:Name="Normal"/>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="SelectionStates">
                                <VisualState x:Name="Selected"/>
                                <VisualState x:Name="Unselected">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ContentElement" Storyboard.TargetProperty="IsHitTestVisible" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="False"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <vsm:VisualStateGroup x:Name="ContentStates">
                                <vsm:VisualState x:Name="Loaded"/>
                                <vsm:VisualState x:Name="Loading">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="LoadingVisual" Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <DoubleAnimation From="0" To="359" Duration="0:0:2" RepeatBehavior="Forever" Storyboard.TargetName="LoadingVisualAngleTransform" Storyboard.TargetProperty="Angle"/>
                                        <DoubleAnimation To="0" Duration="0" Storyboard.TargetName="ContentElement" Storyboard.TargetProperty="Opacity"/>
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="InvalidContent">
                                    <Storyboard Duration="0">
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ErrorElement" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame Value="Visible" KeyTime="0"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                        </vsm:VisualStateManager.VisualStateGroups>
                        <telerikPrimitives:LayoutTransformControl x:Name="layoutTransform">
                            <Grid Background="Transparent">
                                <ContentPresenter x:Name="ContentElement" Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}"/>
                                <Grid x:Name="LoadingVisual" RenderTransformOrigin="0.5,0.5" Visibility="Collapsed" VerticalAlignment="Center" HorizontalAlignment="Center" MaxWidth="50" MaxHeight="50">
                                    <Grid.RenderTransform>
                                        <RotateTransform x:Name="LoadingVisualAngleTransform" Angle="0" CenterX="0.5" CenterY="0.5"/>
                                    </Grid.RenderTransform>
                                    <Path
                                            Stretch="UniformToFill"
                                            StrokeThickness="6"
                                            StrokeStartLineCap="Flat"
                                            StrokeEndLineCap="Triangle"
                                            Data="M1,0 A1,1,90,1,1,0,-1"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Stroke="{telerik1:VisualStudio2013Resource ResourceKey=AccentBrush}"/>
                                </Grid>
                                <Image x:Name="ErrorElement"
                                        Stretch="None"
                                        Visibility="Collapsed"
                                        Source="/Telerik.Windows.Controls.Navigation;component/images/CoverFlowError.png"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"/>
                            </Grid>
                        </telerikPrimitives:LayoutTransformControl>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="CacheMode">
            <Setter.Value>
                <BitmapCache/>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>
